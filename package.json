{
  "name": "ng-gist",
  "version": "1.0.2",
  "description": "AOT/JIT Angular component to embed Gist in your application",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivateOrInternalSupport",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint --project \"./tsconfig.json\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwannabebot/ng-gist.git"
  },
  "keywords": [
    "angular",
    "angular4",
    "gist"
  ],
  "author": {
    "name": "iwannabebot",
    "url": "www.iwannabebot.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iwannabebot/ng-gist/issues"
  },
  "homepage": "https://github.com/iwannabebot/ng-gist#ng-gist",
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/core": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "^3.3.13",
    "@types/karma": "^3.0.3",
    "@types/node": "^12.0.4",
    "@types/webpack": "^4.4.32",
    "@types/webpack-env": "^1.13.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "codecov": "^3.5.0",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-inline-ng2-template": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^3.4.0",
    "json-loader": "^0.5.4",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "node-sass": "^4.5.2",
    "raw-loader": "^2.0.0",
    "rimraf": "2.6.3",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "ts-node": "^8.2.0",
    "tslint": "^5.2.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^2.0.0",
    "zone.js": "^0.9.1"
  }
}
